    "@stripe/react-stripe-js": "^1.7.0",
    "@stripe/stripe-js": "^1.22.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "compression": "^1.7.3",
    "css-loader": "^6.5.1",
    "express": "^4.16.4",
    "faker": "^6.6.6",
    "history": "^4.9.0",
    "html-webpack-plugin": "^5.5.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "^1.9.1",
    "pg": "^8.5.1",
